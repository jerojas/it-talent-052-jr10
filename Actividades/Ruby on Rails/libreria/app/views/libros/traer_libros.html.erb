<script>
		function getLibros(){
			var libros = [];
	$.ajax({

	url: 'http://localhost:3000/servicio',
	method: 'get'
}).done(function(response){
	
	libros = response.libros;
	body = document.getElementById('body')
	libros.forEach(function(libro){

		console.log(libro);

		tr = document.createElement('tr')
		td = document.createElement('td');
		td2 = document.createElement('td2');
		td.append(libro.titulo);
		td2.append(libro.descripcion);

		tr.appendChild(td)
		tr.appendChild(td2)

		body.appendChild(tr)

		


	


	});
})

		
}


</script>
</body>
